{
    "name": "klaatoo-explorer",
    "version": "0.1.2",
    "description": "A Block Explorer for klaatoo",
    "private": true,
    "license": "MIT",
    "scripts": {
        "start": "webpack serve --config webpack/start.js",
        "build": "webpack --config webpack/build.js",
        "fetch:i18n": "node scripts/fetch.i18n.js"
    },
    "devDependencies": {
        "@babel/cli": "7.14.3",
        "@babel/core": "7.14.3",
        "@babel/preset-env": "7.14.2",
        "autoprefixer": "10.2.5",
        "babel-loader": "8.2.2",
        "clean-webpack-plugin": "3.0.0",
        "css-loader": "5.2.6",
        "eslint": "^8.7.0",
        "eslint-config-google": "^0.14.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-svelte3": "^3.4.0",
        "file-loader": "6.2.0",
        "google-spreadsheet": "^3.2.0",
        "html-webpack-plugin": "5.3.1",
        "mini-css-extract-plugin": "1.6.0",
        "postcss": "^8.4.5",
        "postcss-cli": "8.3.1",
        "postcss-import": "14.0.2",
        "postcss-loader": "5.3.0",
        "postcss-preset-env": "6.7.0",
        "prerender-spa-plugin": "^3.4.0",
        "prettier": "2.3.0",
        "purgecss-webpack-plugin": "^4.1.3",
        "size-plugin": "2.0.2",
        "style-loader": "2.0.0",
        "svelte-loader": "3.1.1",
        "svelte-preprocess": "4.7.3",
        "tailwindcss": "2.1.2",
        "webpack": "5.37.1",
        "webpack-cli": "4.9.0",
        "webpack-dev-server": "3.11.2",
        "webpack-manifest-plugin": "^5.0.0",
        "webpack-merge": "5.7.3"
    },
    "dependencies": {
        "chart.js": "^3.5.0",
        "copy-webpack-plugin": "^9.0.1",
        "css-minimizer-webpack-plugin": "^3.3.1",
        "glob": "^7.2.0",
        "moment": "^2.29.1",
        "sockjs-client": "^1.5.2",
        "svelte": "^3.45.0",
        "svelte-chartjs": "^1.1.2",
        "svelte-i18n": "^3.3.13",
        "svelte-meta": "^1.0.2",
        "svelte-routing": "^1.6.0",
        "uglify-js": "^3.14.5"
    },
    "babel": {
        "presets": [
            "@babel/preset-env"
        ]
    },
    "browserslist": [
        "last 3 chrome version",
        "last 3 firefox version",
        "last 3 safari version",
        "last 3 ios_saf version"
    ],
    "postcss": {
        "plugins": {
            "postcss-preset-env": {},
            "postcss-import": {},
            "tailwindcss": {}
        }
    },
    "prettier": {
        "printWidth": 80,
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "none",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "arrowParens": "avoid"
    }
}
